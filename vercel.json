{
    "version": 2,
    "name": "financian-server",
    "buildCommand": "npm run deploy",
    "devCommand": "nom run dev",
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.js"
        }
    ]
}