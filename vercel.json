{
    "version": 2,
    "name": "financian-server",
    "buildCommand": "npm run deploy",
    "devCommand": "nom run dev",
    "builds": [
        {
            "src": "./src/index.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "./src/index.ts"
        }
    ]
}