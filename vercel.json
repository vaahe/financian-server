{
    "version": 2,
    "name": "financian-server",
    "builds": [
        {
            "src": "dist/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/dist/index.js"
        }
    ],
    "build": "npm run deploy",
    "installCommand": "npm install"
}